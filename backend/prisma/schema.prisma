generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model Person {
    id              String @id @default(uuid())
    name            String
    degree          String
    birth_year      number
    email           String
    phone           number
    insurace_number number
    insurance_id    String
    address_id      String
    password        String
}

model InsuranceCompany {
    id      String @id @default(uuid())
    number  number
    name    String
}
